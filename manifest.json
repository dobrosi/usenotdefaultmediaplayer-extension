{
  "manifest_version": 2,
  "version": "1.0.1",

  "name": "notdefaultmediaplayer",
  "description": "Use not default media player.",

  "browser_action": {
    "default_popup": "index.html"
  },
 "content_scripts": [{
        "matches":    ["*://*/*"],
        "js":         ["content.js"]
    }],
  "permissions": ["tabs"]
}
